:javascript
  window.addEventListener("chatwoot:ready", function () { 
    var emailUser = '#{current_tutor.email}'
    var nameUser = '#{current_tutor.name}'
    var hasssh = '#{current_tutor.identifier_string}'
    var identifier = '#{current_tutor.id}-tutor'

    window.$chatwoot.setUser(identifier, {
      email: emailUser,
      name: nameUser,
      identifier_hash: hasssh,
    });

    window.$chatwoot.setCustomAttributes({
      accountId: '#{current_tutor.id}-tutor',
    });
  });

  window.chatwootSettings = {
    position: "left"
    type: "standard",
    launcherTitle: "",
  };
  
  (function(d,t) {
    var g=d.createElement(t),s=d.getElementsByTagName(t)[0];
    g.src=window.location.origin+"/packs/js/sdk.js";
    g.defer = true;
    g.async = true;
    s.parentNode.insertBefore(g,s);
    g.onload=function(){
      window.chatwootSDK.run({
        websiteToken: 'Gfm9QnBUJk6qXtCvNMMBLWZY',
        baseUrl: window.location.origin
      });
    }
  })(document,"script");